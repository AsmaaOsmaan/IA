@model IEnumerable<WebApplication10.Models.projects>

@{
    ViewBag.Title = "listprojects";
    //Layout ="~/views/shared/_directorlayout.cshtml";
    Layout = "~/views/shared/_adminlayout.cshtml";

}

<h2>Projects Displayed</h2>


<table class="table" style="text-align:start">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.titel)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.description)
        </th>
        @*<th>
            @Html.DisplayNameFor(model => model.time_)
        </th>*@
       
        @*<th>
            @Html.DisplayNameFor(model => model.users.email)
        </th>*@
        @*<th>
            @Html.DisplayNameFor(model => model.status.name_)
        </th>*@
        <th>  action</th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.titel)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.description)
        </td>
        @*<td>
            @Html.DisplayFor(modelItem => item.time_)
        </td>*@
       
        @*<td>
            @Html.DisplayFor(modelItem => item.users.email)
        </td>*@
        @*<td>
            @Html.DisplayFor(modelItem => item.status.name_)
        </td>*@
        <td>
           

            @Html.ActionLink("sendrequest", "sendrequest", new { id = item.id }, new { @class = "btn btn-primary" }) 
        </td>
    </tr>
}

</table>
