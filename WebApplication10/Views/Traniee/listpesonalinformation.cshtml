@model IEnumerable<WebApplication10.Models.users>

@{
    ViewBag.Title = "listpesonalinformation";
    Layout = "~/Views/shared/_customerlayout.cshtml";
}

<h2>listpesonalinformation</h2>


<table class="table" style="text-align:start">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.firstname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.lastname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.phone)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.photo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.jobDescription)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UserRole.titleRole)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.firstname)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.lastname)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.phone)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.photo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.jobDescription)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UserRole.titleRole)
        </td>
        <td>
            @Html.ActionLink("Check requests", "requests", new { id=item.Id }, new { @class = "btn btn-default" }) 
            @Html.ActionLink("show current projects", "current_view", null, new { @class = "btn btn-primary" })
            @Html.ActionLink("show previous projects", "previous", null, new { @class = "btn btn-default" })



            

            
          
        </td>
    </tr>
}

</table>
<h2 style="text-align:left">
    @Html.ActionLink("Create New", "Create")
</h2>