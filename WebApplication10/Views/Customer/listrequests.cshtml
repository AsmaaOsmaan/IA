@model IEnumerable<WebApplication10.Models.projectRequest>

@{
    ViewBag.Title = "listrequests";
    Layout = "~/Views/shared/_customerlayout.cshtml";
}

<h2 >listrequests</h2>


<table class="table">
    <tr>
        @*<th>
            @Html.DisplayNameFor(model => model.time)
        </th>*@
       
        <th>
            @Html.DisplayNameFor(model => model.projects.titel)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.users.email)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        @*<td>
            @Html.DisplayFor(modelItem => item.time)
        </td>*@
       
        <td>
            @Html.DisplayFor(modelItem => item.projects.titel)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.users.email)
        </td>
        <td>
            @Html.ActionLink("Accept", "AcceptRequest", new { id=item.Id }, new { @class = "btn btn-success" }) 
            @Html.ActionLink("Reject", "rejectRequest", new { id=item.Id }, new { @class = "btn btn-danger" }) 
           
        </td>
    </tr>
}

</table>
<h2 style="text-align:left">
    @Html.ActionLink("Create New", "Create")
</h2>